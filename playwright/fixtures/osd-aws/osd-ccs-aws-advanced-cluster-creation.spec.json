      "NodeCount": "2",
      "ComputeNodeCount": "4",
      "MinimumNodeCount": "3",
      "MaximumNodeCount": "4",
      "NodeLabel": [
        {
          "Key": "playwright-tests-key",
          "Value": "cyp-osd-ccs-aws-pub-adv"
        },
        {
          "Key": "Available-zone",
          "Value": "Multi-zone"
        }
      ],
      "Taints": [
        {
          "Key": "playwright-tests-key",
          "Value": "ccs-advanced",
          "Effect": "NoExecute"
        },
        {
          "Key": "playwright-tests-taints",
          "Value": "multi-zone",
          "Effect": "NoSchedule"
        }
      ]
    }
  ],
  "CustomApplicationIngress": "Custom settings",
  "ApplySameSecurityGroupsToAllNodeTypes": "true",
  "RouteSelector": {
    "KeyValue": "playwright-tests-routerkey=cyp-osd-ccs-aws-public"
  },
  "ExcludedNamespaces": {
    "Values": "availability,multi-zone,namespaces"
  },
  "WildcardPolicy": "Disallowed",
  "NamespaceOwnershipPolicy": "Strict namespace ownership",
  "ClusterPrivacy": "Public",
  "InstallIntoExistingVPC": "Enabled",
  "MachineCIDR": "10.0.0.0/16",
  "ServiceCIDR": "172.30.0.0/16",
  "PodCIDR": "10.128.0.0/14",
  "HostPrefix": "/23",
