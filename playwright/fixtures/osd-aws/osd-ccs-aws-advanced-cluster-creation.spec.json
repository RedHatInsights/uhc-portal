    "Support": {
      "notificationContact": {
        "username": "ocmui-orgadmin",
        "firstName": "ocmui",
        "lastName": "admin"
      },
      "validations": {
        "invalidUsername": "admin$test",
        "invalidUsernameError": "Username includes illegal symbols",
        "nonExistentUsername": "admintest",
